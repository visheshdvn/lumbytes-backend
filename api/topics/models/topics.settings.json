{
  "kind": "collectionType",
  "collectionName": "topics",
  "info": {
    "name": "topics"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "topicname": {
      "type": "string",
      "required": true
    },
    "theme": {
      "type": "string",
      "default": "3883CA",
      "required": true
    },
    "metaDescription": {
      "type": "string",
      "required": true
    },
    "headImg": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "blogposts": {
      "collection": "blogposts",
      "via": "topic"
    },
    "secondaryPosts": {
      "collection": "blogposts",
      "via": "subTopic"
    }
  }
}
